    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@rollup/plugin-alias": "^3.1.1",
    "bitcoinjs-lib": "^5.2.0",
    "bs58": "^4.0.1",
    "did-jwt": "^4.8.1",
    "did-resolver": "^2.1.2",
    "is-promise": "^4.0.0",
    "node-localstorage": "^2.1.6",
